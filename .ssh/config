# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host github.com
	Hostname github.com
	User git
	IdentityFile ~/.ssh/id_ed25519

Host work.github.com
	Hostname github.com
	User git
	IdentityFile ~/.ssh/purplease

Host sms
	Hostname 74.208.14.89
	User root
	IdentityFile ~/.ssh/ionos_vps

Host qdev
	Hostname ec2-3-91-235-24.compute-1.amazonaws.com 
	User ec2-user
	IdentityFile ~/.ssh/qdev

Host qprod
	Hostname ec2-35-171-151-70.compute-1.amazonaws.com
	User ec2-user
	IdentityFile ~/.ssh/qprod
